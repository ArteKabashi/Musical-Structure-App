<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.mymusicapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/mixtape"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:fullBackupContent="@xml/backup_descriptor">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Music"
            android:label="Songs"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".PlayMusic"
            android:label="Playing"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".MusicPlaylist"
            android:label="Playlist"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Settingss"
            android:label="Settings"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Youtube"
            android:label="Youtube Links"
            android:parentActivityName=".MainActivity"></activity>
    </application>
</manifest>